animation.html
<!DOCTYPE html>
<html>
    <head>
        <style>
            @keyframes demo{
                /* from{background-color: pink;} 
                to{background-color: green;} */
                0% {background-color: pink;}
                /* 25%{ background-color: cyan;}
                50% {background-color: yellow;}
                75%{background-color: red;} */
                100%{background-color: blue; top: 0px; left: 100px;}
            }
            /* div{height: 200px; width: 200px; background-color:pink;animation-name: demo; animation-duration: 5s; animation-delay: 2s; 
                animation-timing-function: linear; animation-iteration-count: infinite; animation-direction: normal; }*/
                div{position:relative;height: 200px; width: 200px; background-color:pink;animation-name: demo; 
                    animation-duration: 5s; animation-delay: 2s; 
                animation-timing-function: linear; animation-iteration-count: infinite; animation-direction: normal; }
        
        </style>
    </head>
    <body>
        <div></div>
    </body>
</html>

example(ani).html
<!DOCTYPE html>
<html>
    <head>
        <style>
            @keyframes demo {
                0%{background-color: cyan; top: 0px;left: 0px;}
                25%{background-color: tomato; top: 0px;left: 500px;}
                50%{background-color: yellow; top: 500px;left: 500px;}
                75%{background-color: green; top: 500px;left: 0px;}
                100%{background-color: cyan; top: 0px;left: 0px;}
            }
            div{position:relative;height:  200px; width: 200px; background-color: cyan; animation-name: demo; 
                animation-timing-function:linear;animation-duration:  5s;
            animation-delay: 2s; animation-direction: reverse; animation-iteration-count: infinite;}
        </style>
    </head>
    <body>
        <div></div>
    </body>
</html>

example2(ani).html
<!DOCTYPE html>
<html>
    <head>
        <style>
            @keyframes circle{
                0%{background-color: cyan;top: 0px;left: 0px;}
                50%{background-color: yellow; top: 500px; left: 250px;border-radius: 50%;}
                100%{background-color: red;top:0px; left:500px;}
            }
            div{position: relative;height: 200px; width: 200px; background-color: cyan;animation: circle 5s linear 2s infinite alternate;}
        </style>
    </head>
    <body>
        <div></div>
    </body>
</html>

Example(10/11).html
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Document</title>
    <style>
        @keyframes box{
            0%{background-color: pink;top: 0px; left: 0px; transform: scaleY(1);}
            25%{background-color: pink;top: 0px;left: 100px; transform: scaleY(1.2);}
            50%{background-color: pink;top: 0px;left: 200px;transform: scaleY(1); transform: scaleX(1);}
            75%{background-color: pink;top: 0px;left: 300px;transform: scaleX(1.2);}
            100%{background-color: pink;top: 0px;left: 400px;transform: scaleX(1);}
        }
        div{position: relative;height: 100px;width: 100px;background-color: pink;animation: box 5s linear 2s infinite ;}
    </style>
</head>
<body>
    <div></div>
</body>
</html>

